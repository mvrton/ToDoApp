{
    "build": {
      "env": {
        "MAVEN_VERSION": "3.8.6"
      },
      "commands": {
        "install": "curl -sL https://archive.apache.org/dist/maven/maven-3/3.8.6/binaries/apache-maven-3.8.6-bin.tar.gz | tar xz -C /tmp && ln -s /tmp/apache-maven-3.8.6/bin/mvn /usr/local/bin/mvn"
      }
    }
  }
  